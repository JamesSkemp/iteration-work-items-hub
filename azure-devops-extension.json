{
    "manifestVersion": 1,
    "id": "iteration-work-items-hub",
    "publisher": "JamesSkemp",
    "version": "1.0.24",
    "name": "Iteration Work Items Hub",
    "description": "Azure DevOps Extension",
    "categories": [
        "Azure Repos",
        "Azure Boards",
        "Azure Pipelines",
        "Azure Test Plans",
        "Azure Artifacts"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "logo.png"
    },
    "content": {
